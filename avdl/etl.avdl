@namespace("com.toptal.etl")

/**
  Etl protocol
 */
protocol etl {

  import idl "platform.avdl";

  @bq-table("companies")
  record Company {
    @origin("com.toptal.platform.Role#id")
    /** A subset of role id consisting of companies only */
    int company_id;

    union {null, @origin("com.toptal.platform.Role#id") int} claimer_id;
  }

} 
